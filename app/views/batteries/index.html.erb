<p id="notice"><%= notice %></p>

<h1>Batteries</h1>

<table>
  <thead>
    <tr>
      <th>Buildingid</th>
      <th>Type</th>
      <th>Status</th>
      <th>Employeeid</th>
      <th>Date of commissioning</th>
      <th>Date of last inspection</th>
      <th>Certificate of operations</th>
      <th>Information</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @batteries.each do |battery| %>
      <tr>
        <td><%= battery.buildingId %></td>
        <td><%= battery.Type %></td>
        <td><%= battery.Status %></td>
        <td><%= battery.EmployeeId %></td>
        <td><%= battery.date_of_commissioning %></td>
        <td><%= battery.date_of_last_inspection %></td>
        <td><%= battery.certificate_of_operations %></td>
        <td><%= battery.information %></td>
        <td><%= battery.notes %></td>
        <td><%= link_to 'Show', battery %></td>
        <td><%= link_to 'Edit', edit_battery_path(battery) %></td>
        <td><%= link_to 'Destroy', battery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Battery', new_battery_path %>
